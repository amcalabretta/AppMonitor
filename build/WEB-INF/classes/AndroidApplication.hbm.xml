<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ing.mobile.model.dto.AndroidApplication" table="ANDROID_APPLICATIONS">
    <id name="id" type="int" column="app_id">
        <generator class="native"></generator>
	</id>
	<property name="packageName" column="pkg_name" type="string"/>
	<property name="appDescription" column="app_description" type="string"/>	
</class>
</hibernate-mapping>