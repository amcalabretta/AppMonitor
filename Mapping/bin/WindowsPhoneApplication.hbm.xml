<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.ing.mobile.model.dto.WindowsPhoneApplication" table="WP_APPLICATIONS">
    <id name="id" type="int" column="app_id">
        <generator class="native"></generator>
	</id>
	<property name="description" column="app_description" type="string"/>
	<property name="code" column="app_code" type="string"/>
	<property name="name" column="app_name" type="string"/>
</class>
</hibernate-mapping>